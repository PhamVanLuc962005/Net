﻿@{
    ViewData["Title"] = "Privacy Policy";
}

<div Pvl="alert alert-info">
    <h1>@ViewData["Title"]</h1>
    <hr />
    <p>Member ID : @Model.PvlMemberId</p>
    <p>Username : @Model.PvlUserName</p>
    <p>Password : @Model.PvlPassword</p>
    <p>Fullname : @Model.PvlFullName</p>
    <p>Email : @Model.PvlEmail</p>
</div>